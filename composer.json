{
    "name": "oro/crm-magento1-connector",
    "description": "OroCRM connector to Magento 1",
    "homepage": "https://github.com/oroinc/crm-magento1-connector.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Oro\\Bridge\\MagentoCalendarCRM\\": "src/Oro/Bridge/MagentoCalendarCRM",
            "Oro\\Bridge\\MagentoCallCRM\\": "src/Oro/Bridge/MagentoCallCRM",
            "Oro\\Bridge\\MagentoTaskCRM\\": "src/Oro/Bridge/MagentoTaskCRM",
            "Oro\\Bridge\\MarketingCRM\\": "src/Oro/Bridge/MarketingCRM",
            "Oro\\Bundle\\MagentoBundle\\": "src/Oro/Bundle/MagentoBundle",
            "Oro\\Bundle\\MagentoDemoDataBundle\\": "src/Oro/Bundle/MagentoDemoDataBundle",
            "Oro\\Bundle\\MagentoTerritoryBundle\\": "src/Oro/Bundle/MagentoTerritoryBundle"
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "require": {
        "oro/crm": "5.0.*",
        "guzzle/guzzle": "3.7.4"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "4.4.*"
        }
    }
}
